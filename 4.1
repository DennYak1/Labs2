#include<stdlib.h>
typedef struct node {
    int val;
    struct node * next;
} node;

void push_start(node ** head, int val) 
{
    node *new_node;
    new_node = (node*)malloc(sizeof(node));

    new_node -> val = val;
    new_node -> next = *head;
    *head = new_node;
}
int main() {

    node * head = NULL;
    head = (node*)malloc(sizeof(node));

    if (head == NULL)
    {
        return 1;
    }
    push_start(&head, 0);
    print_list(head);
    return 0;
}
